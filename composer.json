{
    "name": "adaptdk/gsv-auth0-provider",
    "description": "This package providers a middleware and some helper functions for connecting with Auth0 and the GSV Auth API.",
    "keywords": [
        "adaptdk",
        "laravel",
        "gsv-auth0-provider"
    ],
    "homepage": "https://github.com/adaptdk/gsv-auth0-provider",
    "license": "MIT",
    "authors": [
        {
            "name": "Jess Stopa",
            "email": "jess@stopa.dk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-package-tools": "^1.4.3",
        "illuminate/contracts": "^8.37"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.9",
        "vimeo/psalm": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "Adaptdk\\GsvAuth0Provider\\": "src",
            "Adaptdk\\GsvAuth0Provider\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Adaptdk\\GsvAuth0Provider\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Adaptdk\\GsvAuth0Provider\\GsvAuth0ProviderServiceProvider"
            ],
            "aliases": {
                "GsvAuth0Provider": "Adaptdk\\GsvAuth0Provider\\GsvAuth0ProviderFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
