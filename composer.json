{
    "name": "adaptdk/gsv-auth0-provider",
    "description": "GSV Auth0 Authentication package",
    "keywords": [
        "adaptdk",
        "laravel",
        "gsv-auth0-provider"
    ],
    "homepage": "https://github.com/adaptdk/gsv-auth0-provider",
    "license": "MIT",
    "authors": [
        {
            "name": "Jess Stopa",
            "email": "jess@adaptagency.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "auth0/auth0-php": "^7.9",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.18",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Adaptdk\\GsvAuth0Provider\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Adaptdk\\GsvAuth0Provider\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit",
        "test-coverage": "./vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Adaptdk\\GsvAuth0Provider\\GsvAuth0ProviderServiceProvider"
            ],
            "aliases": {
                "GsvAuth0Provider": "Adaptdk\\GsvAuth0Provider\\GsvAuth0ProviderFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
